        "print(\"--- Soal 7 ---\")\n",
        "bulan = [\"Januari\", \"Februari\", \"Maret\", \"April\"]\n",
        "\n",
        "for i, nama in enumerate(bulan):\n",
        "    # i = indeks (dimulai dari 0)\n",
        "    # nama = elemen (nama bulan)\n",
        "    print(f\"Bulan ke-{i + 1}: {nama}\")\n"
      ],
      "metadata": {
