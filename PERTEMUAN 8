      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from numpy import nan\n",
        "\n",
        "# asumsikan nama dataframe kamu adalah df\n",
        "muhammad_syawaludin['umur'] = muhammad_syawaludin['umur'].replace(['21years', '21 years'], 21)\n",
        "muhammad_syawaludin['umur'] = muhammad_syawaludin['umur'].astype(float)\n",
        "\n",
        "# perbaiki nilai umur yang negatif\n",
        "muhammad_syawaludin.loc[muhammad_syawaludin['umur'] < 0, 'umur'] = muhammad_syawaludin['umur'].median()\n",
        "\n",
        "# batas logis umur mahasiswa 18-30\n",
        "muhammad_syawaludin.loc[muhammad_syawaludin['umur'] > 30, 'umur'] = 30\n",
        "\n",
        "# perbaiki nilai kehadiran\n",
        "muhammad_syawaludin['kehadiran_pct'] = muhammad_syawaludin['kehadiran_pct'].clip(lower=0, upper=100)\n",
        "\n",
        "# nilai akhir < 0 dianggap tidak valid  minimal 0\n",
        "muhammad_syawaludin['nilai_akhir'] = muhammad_syawaludin['nilai_akhir'].clip(lower=0, upper=100)\n",
        "\n",
        "# perbaikan format tanggal\n",
        "muhammad_syawaludin['tanggal_daftar'] = pd.to_datetime(muhammad_syawaludin['tanggal_daftar'], errors='coerce').dt.strftime('%Y-%m-%d')\n",
        "\n",
        "# ganti NaN string pada jurusan\n",
        "muhammad_syawaludin['jurusan'] = muhammad_syawaludin['jurusan'].replace('NaN_string', nan)\n",
        "muhammad_syawaludin['jurusan'] = muhammad_syawaludin['jurusan'].fillna(muhammad_syawaludin['jurusan'].mode()[0])  # isi dengan jurusan terbanyak\n",
        "\n",
        "# isi umur yang NaN dengan rata-rata\n",
        "muhammad_syawaludin['umur'] = muhammad_syawaludin['umur'].fillna(muhammad_syawaludin['umur'].median())\n",
        "\n",
        "print(muhammad_syawaludin.head(70))\n"
      ],
