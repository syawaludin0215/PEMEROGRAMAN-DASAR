        "monthly_orders_df = all_df.resample(rule='M', on='order_date').agg({\n",
        "    \"order_id\": \"nunique\",\n",
        "    \"total_price\": \"sum\"\n",
        "})\n",
        "monthly_orders_df.index = monthly_orders_df.index.strftime('%Y-%m')\n",
